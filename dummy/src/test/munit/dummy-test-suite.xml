<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:munit="http://www.mulesoft.org/schema/mule/munit" xmlns:munit-tools="http://www.mulesoft.org/schema/mule/munit-tools"
	xmlns="http://www.mulesoft.org/schema/mule/core" xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xsi:schemaLocation="
		http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
		http://www.mulesoft.org/schema/mule/munit http://www.mulesoft.org/schema/mule/munit/current/mule-munit.xsd
		http://www.mulesoft.org/schema/mule/munit-tools  http://www.mulesoft.org/schema/mule/munit-tools/current/mule-munit-tools.xsd">
	<munit:config name="dummy-test-suite.xml" />
	<munit:test name="dummy-test-suite-dummyFlowTest" doc:id="e204e2bc-110b-4ce5-9e24-66c6f199a9e4" description="Test">
		<munit:execution >
			<flow-ref doc:name="Flow-ref to dummyFlow" doc:id="31c83346-9c55-4e03-8cd6-e49fedf012e0" name="dummyFlow"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-equals doc:name="Assert equals" doc:id="591eb72e-2eaf-40bb-a886-c3e3d1244340" actual="#[payload]" expected='#["hello dummy!"]' message='#["payload does not match with expected"]'/>
		</munit:validation>
	</munit:test>
	<munit:test name="dummy-test-suite-dummyFlowTest1" doc:id="0a86da1c-5cb3-43c5-b4b1-15f6e9688ea1" description="Test">
		<munit:execution >
			<flow-ref doc:name="Flow-ref to dummyFlow" doc:id="f3ee080d-b8bb-4bd8-8fb4-f436c92cb6ac" name="dummyFlow"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-that doc:name="Assert that" doc:id="897dbebd-0778-4000-887b-c3642ed170bf" message='#["payload does not starts with hello."]' expression="#[payload]" is="#[MunitTools::startsWith('hello')]"/>
		</munit:validation>
	</munit:test>


</mule>
